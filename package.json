{
  "name": "@dcic/signature-commons-metadata-api",
  "version": "1.4.1",
  "description": "Signature Commons Metadata API Micro Service",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "dotenv-load lb-tsc --outDir dist",
    "build:watch": "dotenv-load lb-tsc --watch",
    "build:docker": "dotenv-load node ./bin/docker-build.js",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "dotenv-load lb-mocha --allow-console-logs \"dist/test\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "dotenv-load node .",
    "prepublishOnly": "npm run test",
    "deploy": "dotenv-load npm run build && npm run deploy:docker",
    "deploy:docker": "npm run build:docker && dotenv-load node ./bin/docker-deploy.js",
    "version:major": "npm version --no-git-tag-version major",
    "version:minor": "npm version --no-git-tag-version minor",
    "version:patch": "npm version --no-git-tag-version patch"
  },
  "repository": {
    "type": "git"
  },
  "author": "Daniel J. B. Clarke (u8sand@gmail.com)",
  "license": "Apache-2.0",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@dcic/signature-commons-schema": "^1.3.8",
    "@loopback/authentication": "^1.2.1",
    "@loopback/boot": "^1.5.3",
    "@loopback/context": "^1.21.4",
    "@loopback/core": "^1.9.3",
    "@loopback/openapi-v3": "^1.9.4",
    "@loopback/repository": "^1.12.0",
    "@loopback/rest": "^1.17.0",
    "@loopback/service-proxy": "^1.3.3",
    "@types/debug": "^4.1.5",
    "@types/uuid": "^3.4.5",
    "cross-env": "^5.2.0",
    "debug": "^4.1.1",
    "deepmerge": "^3.3.0",
    "dotenv": "^8.1.0",
    "loopback-connector-postgresql": "github:u8sand/loopback-connector-postgresql#fullTextSearch",
    "memory": "0.0.3",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "reflect-metadata": "^0.1.13",
    "serialize-error": "^4.1.0",
    "typeorm": "^0.3.0-alpha.23",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@loopback/build": "^2.0.8",
    "@loopback/testlab": "^1.7.4",
    "@types/assert": "^1.4.3",
    "@types/node": "^10.14.16",
    "@types/passport": "^0.4.7",
    "@types/passport-http": "^0.3.8",
    "assert": "^1.5.0",
    "dotenv-load": "^2.0.0",
    "shelljs": "^0.8.3",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
}
