{
  "name": "default",
  "type": "postgres",
  "url": "posgresql://root:pass@localhost:5432/testdb",
  "synchronize": true,
  "logging": true,
  "entities": [
    "dist/entities/**/*.js"
  ],
  "migrations": [
    "dist/migration/**/*.js"
